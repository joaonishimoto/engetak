generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  MEMBER
  ADMIN
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  password String
  role     Role   @default(MEMBER)

  @@map("users")
}
